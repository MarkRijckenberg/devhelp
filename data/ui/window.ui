<!--*- xml -*-->
<ui>
  <menubar name="MenuBar">
    <menu action="FileMenu">
      <menuitem action="NewTab"/>
      <separator/>
      <menuitem action="Print"/>
      <separator/>
      <menuitem action="Close"/>
    </menu>
    <menu action="EditMenu">
      <menuitem action="Copy"/>
      <separator/>
      <menuitem action="Find"/>
      <menuitem action="Find Next"/>
      <menuitem action="Find Previous"/>
    </menu>
    <menu action="ViewMenu">
      <menuitem action="ZoomIn"/>
      <menuitem action="ZoomOut"/>
      <menuitem action="ZoomDefault"/>
      <separator/>
      <menuitem name="ViewFullscreenMenu" action="ViewFullscreen"/>
    </menu>
    <menu action="GoMenu">
      <menuitem action="Back"/>
      <menuitem action="Forward"/>
      <separator/>
      <menuitem action="ShowSearchTab"/>
      <menuitem action="ShowContentsTab"/>
    </menu>
  </menubar>

  <toolbar name="Toolbar">
    <toolitem action="Back"/>
    <toolitem action="Forward"/>
    <separator/>
    <toolitem action="ZoomOut"/>
    <toolitem action="ZoomIn"/>
  </toolbar>

  <toolbar name="FullscreenToolBar">
    <toolitem action="Back"/>
    <toolitem action="Forward"/>
    <separator/>
    <toolitem action="ZoomOut"/>
    <toolitem action="ZoomIn"/>
    <separator expand="true"/>
    <toolitem action="LeaveFullscreen"/>
  </toolbar>

</ui>
